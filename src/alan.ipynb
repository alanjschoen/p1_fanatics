    "from os import listdir\n",
    "import pandas as pd\n",
    "\n",
    "datafiles = ['../data/' + x for x in listdir('../data/')]\n",
    "\n",
    "list_ = []\n",
    "for file_ in datafiles:\n",
    "    df = pd.read_csv(file_)\n",
    "    list_.append(df)\n",
    "frame = pd.concat(list_)\n",
    "\n",
    "frame.head()"
   ]
